{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.compileCommands": "${workspaceFolder}/compile_commands.json",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/inc/**",
        "${workspaceFolder}/inc/base/**",
        "${workspaceFolder}/inc/mcu/esp32/**",
        "${workspaceFolder}/inc/mcu/esp32/utils/**",
        "${workspaceFolder}/inc/utils/**",
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/src/mcu/**",
        "${workspaceFolder}/src/mcu/esp32/**",
        "${workspaceFolder}/src/utils/**",
        "${workspaceFolder}/examples/esp32/**",
        "${workspaceFolder}/examples/esp32/main/**",
        "${env:IDF_PATH}/components/**",
        "${env:HOME}/.espressif/tools/riscv32-esp-elf/esp-15.1.0_20250607/riscv32-esp-elf/riscv32-esp-elf/include/**",
        "${env:HOME}/.espressif/tools/riscv32-esp-elf/esp-15.1.0_20250607/riscv32-esp-elf/lib/gcc/riscv32-esp-elf/15.1.0/include/**",
        "${env:HOME}/.espressif/tools/riscv32-esp-elf/esp-14.2.0_20241119/riscv32-esp-elf/riscv32-esp-elf/include/**",
        "${env:HOME}/.espressif/tools/riscv32-esp-elf/esp-14.2.0_20241119/riscv32-esp-elf/lib/gcc/riscv32-esp-elf/14.2.0/include/**"
    ],
    "C_Cpp.default.browse.path": [
        "${workspaceFolder}/**",
        "${env:IDF_PATH}/components/**"
    ],
    "C_Cpp.default.defines": [
        "ESP_PLATFORM=1",
        "CONFIG_IDF_TARGET_ESP32C6=1",
        "HF_TARGET_MCU_ESP32C6=1",
        "HF_MCU_ESP32C6=1",
        "HF_MCU_FAMILY_ESP32=1",
        "HF_THREAD_SAFE=1",
        "ESP32C6=1"
    ],
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.cppStandard": "gnu++17",
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    "files.associations": {
        "*.h": "c",
        "*.hpp": "cpp",
        "*.cpp": "cpp",
        "*.c": "c",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "random": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "typeinfo": "cpp"
    },
    "cmake.sourceDirectory": "${workspaceFolder}/examples/esp32",
    "cmake.buildDirectory": "${workspaceFolder}/examples/esp32/build_pwm_test_Release",
    "search.exclude": {
        "**/build_*": true,
        "**/venv": true,
        "**/.espressif": true,
        "**/esp-idf": true
    },
    "files.watcherExclude": {
        "**/build_*/**": true,
        "**/venv/**": true,
        "**/.espressif/**": true,
        "**/esp-idf/**": true
    },
    "C_Cpp.loggingLevel": "Information",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.errorSquiggles": "enabled"
}
